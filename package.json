{
  "name": "websftp",
  "version": "1.0.0",
  "description": "Outil de gestion de projets Node.js pour Ubuntu 22.04 avec SFTP sécurisé et interface Web",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "project-manager": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "web": "node src/web/server.js",
    "web:dev": "node --watch src/web/server.js",
    "update": "git pull origin main && npm install"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "express": "^4.18.2",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1",
    "cli-table3": "^0.6.3",
    "boxen": "^7.1.1",
    "figlet": "^1.7.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "mongodb": "^6.3.0"
  },
  "keywords": [
    "nodejs",
    "pm2",
    "sftp",
    "ubuntu",
    "project-manager",
    "devops"
  ],
  "author": "Antoine601",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Antoine601/WebSFTP.git"
  },
  "bugs": {
    "url": "https://github.com/Antoine601/WebSFTP/issues"
  },
  "homepage": "https://github.com/Antoine601/WebSFTP#readme"
}
